{
    "name": "api",
    "version": "1.0.0",
    "description": "Cloudflare Worker API for Auth Service (Vanilla JavaScript)",
    "main": "src/index.js",
    "type": "module",
    "scripts": {
        "dev": "wrangler dev",
        "deploy": "wrangler deploy",
        "db:migrate": "wrangler d1 execute auth-db --file=./migrations/0001_initial.sql",
        "db:seed": "wrangler d1 execute auth-db --file=./seeds/dev-data.sql"
    },
    "dependencies": {
        "hono": "^4.0.0",
        "jose": "^5.2.0"
    },
    "devDependencies": {
        "wrangler": "^3.22.1"
    }
}